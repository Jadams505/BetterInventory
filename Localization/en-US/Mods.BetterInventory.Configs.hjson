Compatibility: {
	DisplayName: Mod Compatibility

	compatibilityMode: {
		Name: Compatibility Mode
		Label: "[i:509] {$Mods.BetterInventory.Configs.Compatibility.compatibilityMode.Name}"
		Tooltip:
			'''
			Disabled features will not be loaded and will have no effect even when Enabled
			Enable this if {$Mods.BetterInventory.Chat.ModName} fails to load or causes crashes when loading other mods

			If you are is this situation:
			1) Enable "{$Mods.BetterInventory.Configs.Compatibility.compatibilityMode.Name}" and initialize "{$Mods.BetterInventory.Configs.Compatibility.DisableAll.Name}"
			2) Reload
			3) Re-enable a feature and Reload. If the loading fails, disable it
			4) Repeat step 3 with every features you want to enable

			Also, please report the mods you had and the features you had to disable on the {$Mods.BetterInventory.Chat.Workshop} or the {$Mods.BetterInventory.Chat.Homepage}
			'''
	}

	DisableAll: {
		Name: Disable All
		Label: "[i:348] {$Mods.BetterInventory.Configs.Compatibility.DisableAll.Name}"
		Tooltip:
			'''
			Disables every feature that could cause a crash when loaded
			[c/ff0000:This action cannot be undone, you will have to re-enable the features manually. Use carefully]
			'''
	}

	UnloadedCrafting: {
		Label: "[i:398] Unloaded {$Mods.BetterInventory.Configs.Crafting.DisplayName} Features"
		Tooltip: ""
	}

	UnloadedInventoryManagement: {
		Label: "[i:5075] Unloaded {$Mods.BetterInventory.Configs.InventoryManagement.DisplayName} Features"
