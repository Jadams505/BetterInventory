InventoryManagement: {
	DisplayName: Inventory Management

	smartConsumption: {
		Label: "[i:42] Smart item consumption"
		Tooltip: Consumes items from the smallest stack in the inventory
	}

	smartAmmo: {
		Label: "[i:40] Smart Ammo Consumption"
		Tooltip: Consumes ammo (arrows, baits, paints, ...) from the last stack in the inventory. Doesn't affect the selected ammo
	}

	smartPickup: {
		Label: "[i:4131] Smart Item Pickup"
		Tooltip:
			'''
			Items go their previous slot in your inventory when picked up
			{$Mods.BetterInventory.Configs.SmartPickupLevel.FavoriteOnly.Label}: Applies to Favorited items only
			{$Mods.BetterInventory.Configs.SmartPickupLevel.AllItems.Label}: Applies to all items
			'''
	}

	autoEquip: {
		Label: "[i:83] {$Mods.BetterInventory.Configs.InventoryManagement.autoEquip.Name}"
		Name: Auto Equip Items
		Tooltip:
			'''
			Automaticaly equips items such as Armors, Accessories or Equipement
			{$Mods.BetterInventory.Configs.AutoEquipLevel.DefaultSlots.Label}: Equipement go to their default slots if available
			{$Mods.BetterInventory.Configs.AutoEquipLevel.AnySlot.Label}: Equipement go to any slot they can go in. Includes Dyes. Armors and Accessories can go to social slots.
			'''
	}

	quickMove: {
		Label: "[i:3620] Quick Move"
		Tooltip: Hover over an item in your inventory and press a hotbar keybind to move it between different contexts
	}

	fastContainerOpening: {
		Label: "[i:5002] Fast Container Opening"
		Tooltip:
			'''
			Hold rick click to rapidly open containers
			Faster Extractinator
			'''
	}

	itemRightClick: {
		Label: "[i:1503] Extra Item Right Click"
		Tooltip: Item that can be right clicked in the inventory can be right clicked when held
	}

	builderKeys: {
		Label: "[i:3061] Builder Keybinds"
		Tooltip: Enables keybinds to toggles the builder toggles
	}

	FavoriteBuffKeybind: {
		Label: "[i:2348] Favorite Quick Buff"
		Tooltip: Similar to Quick Buff, but only consumes favorite items
	}

	favoriteInBanks: {
		Label: "[i:75] Favorite Items in Personal Storages"
		Tooltip: Allows you to favorite items in your personal storages
	}

	clickOverrides: {
		Label: "[i:5075] Click Overrides"
		Tooltip: Makes shops and crafting consistent with inventory movements
	}

	craftStack: {
		Label: craft Stack
		Tooltip: ""
	}

	shiftClick: {
		Label: shift Click
		Tooltip: ""
	}

	stackTrash: {
		Label: stack Trash
		Tooltip: ""
	}
}

