ClickOverride: {
	Tooltip: ""

	crafting: {
		Label: Crafting
		Tooltip:
			'''
			Left Click to craft a stack of items
			Right Click to craft the recipe once
			Hold Shift to tranfer the crafted items in your inventory
			'''
	}

	shops: {
		Label: Shops
		Tooltip:
			'''
			Left Click to buy a stack of items
			Right Click to buy once
			Hold Shift to tranfer the baught items in your inventory
			'''
	}

	shiftRight: {
		Label: Shift Right Click
		Tooltip: Allows you to Shift or Control Right Click to transfer one item
	}

	stacking: {
		Label: Trash and Shops Stacking
		Tooltip:
			'''
			Items sold multiple times will stack a single one slot
			Items trashed multiple in a row times will stack instead of replacing the old one
			'''
	}

	invertClicks: {
		Label: Invert Clicks
		Tooltip: Invert the behaviour of the left and right clicks when crafting or buying items
	}
}

QuickMove: {
	Tooltip: ""

	chainTime: {
		Label: Chain Interval
		Tooltip: Frame interval until the current chain resets (60 = 1s)
	}

	showTooltip: {
		Label: Show Tooltip
		Tooltip: Displays the context chain in the item tooltip
	}

	returnToSlot: {
		Label: Return to Original Slot
		Tooltip: Return the item to its original slot when swapping at the end of the chain
	}

	hotkeyMode: {
		Label: Hotkey Mapping
		Tooltip:
			'''
			Changes the mapping of hotkeys depending of the number of target slots
			{$Mods.BetterInventory.Configs.HotkeyMode.Default.Label}: Hotbar #1 maps to slot 1, Hotbar #10 to slot 10
			{$Mods.BetterInventory.Configs.HotkeyMode.FromEnd.Label}: Hotbar #10 maps to the last slot, Hotbar #1 to 10 slot from the end
			{$Mods.BetterInventory.Configs.HotkeyMode.Reversed.Label}: Hotbar #10 maps to slot 1, Hotbar #1 to slot 10
			'''
	}

	displayHotkeys: {
		Label: Display Hotkeys
		Tooltip:
			'''
			Displays hotkeys on the slots where an item can be moved to
			{$Mods.BetterInventory.Configs.HotkeyDisplayMode.First.Label}: Displays the hotkeys of the next context in the chain
			{$Mods.BetterInventory.Configs.HotkeyDisplayMode.All.Label}: Displays the hotkey of the all contexts in the chain
			'''
	}
}

