		Label: "[i:83] Auto Equip Items"
		Tooltip: Automatically equips items such as Armors, Accessories or Equipment
	}

	quickMove: {
		Label: "[i:3620] Quick Move"
		Tooltip:
			'''
			Quickly move items between special slots in the inventory
			Hover over an item in your inventory or equipment and press a hotbar key to move it to this slot
			Press the same key to move to the next special slots in the cycle
			'''
	}

