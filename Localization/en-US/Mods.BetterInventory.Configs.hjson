ItemSearch: {
	DisplayName: Item Search

	betterGuide: {
		Label: "[i:267] {$Mods.BetterInventory.Configs.ItemSearch.betterGuide.Name}"
		Name: Better Guide
		Tooltip: Improves various aspects of the Guide
	}

	betterBestiary: {
		Label: "[i:4767] {$Mods.BetterInventory.Configs.ItemSearch.betterBestiary.Name}"
		Name: Better Bestiary
		Tooltip: Improves various aspects of the Bestiary
	}

	searchRecipes: {
		Label: "[i:3549] {$Mods.BetterInventory.Configs.ItemSearch.searchRecipes.Name}"
		Name: Search Recipes
		Tooltip:
			'''
			Allows you to access the guide anywhere
			Hold the "{$Mods.BetterInventory.Configs.ItemSearch.SearchItemKeybind.Name}" key and Left Click an item to search its recipe
			Hold the "{$Mods.BetterInventory.Configs.ItemSearch.SearchItemKeybind.Name}" key and Middle Click in the inventory to clear the searched item
			With {$Mods.BetterInventory.Configs.BetterGuide.guideTile.Label} On, searching the guide tile will move it into the guide item and vice-versa
			Right Click the Guide item to clear it
			'''
	}

	searchDrops: {
		Label: "[i:2903] {$Mods.BetterInventory.Configs.ItemSearch.searchDrops.Name}"
		Name: Search Drops
		Tooltip:
			'''
			Hold the "{$Mods.BetterInventory.Configs.ItemSearch.SearchItemKeybind.Name}" key and Right Click an item to search its drops
			Hold the "{$Mods.BetterInventory.Configs.ItemSearch.SearchItemKeybind.Name}" key and Middle Click in the Bestiary to clear the searched item
			Right Click Bestiary text field to clear it
			'''
	}

	searchHistory: {
		Label: "[i:346] Search History"
		Tooltip:
			'''
			Right Click the Guide item or Bestiary text field to search the previous value instead of clearing it
			Requires "{$Mods.BetterInventory.Configs.ItemSearch.searchRecipes.Name}" and "{$Mods.BetterInventory.Configs.ItemSearch.searchDrops.Name}" respectively to be On
			'''
	}

	unknownDisplay: {
		Label: "[i:236] Unknown Recipe or NPCs display"
		Tooltip:
			'''
			Changes how Recipes and NPCs you have not discovered are displayed in the Recipe list and Bestiary
			Requires "{$Mods.BetterInventory.Configs.ItemSearch.betterGuide.Name}" and "{$Mods.BetterInventory.Configs.ItemSearch.betterBestiary.Name}" respectively to be On
			{$Mods.BetterInventory.Configs.UnknownDisplay.Hidden.Label}: Not displayed
			{$Mods.BetterInventory.Configs.UnknownDisplay.Unknown.Label}: Displayed as '?'
			{$Mods.BetterInventory.Configs.UnknownDisplay.Known.Label}: Displayed as discovered.
			'''
	}

	SearchItemKeybind: {
		Label: "[i:487] {$Mods.BetterInventory.Configs.ItemSearch.SearchItemKeybind.Name}"
		Name: Search Item
		Tooltip:
			'''
			Tap this key to close the current menu or open the Recipe list
			Double tap this key to open the Bestiary
			'''
	}
}

